{
  "name": "whatsapwebsocket",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "prod:server": "set \"NODE_ENV=developer\" && set \"PRODUCTION_LOCAL=true\" && ts-node-dev --inspect --transpile-only --ignore-watch node_modules  src/app.ts",
    "prod:macserver": " export NODE_ENV=developer && export PRODUCTION_LOCAL=true  && ts-node-dev --inspect --transpile-only --ignore-watch node_modules  src/app.ts",
    "scss": "node compile-scss.js",
    "scss:watch": "sass --watch src/public/assets/scss/connectfy/style.scss src/public/assets/css/connectfy/style.css",
    "taiwind:watch": "npx tailwindcss -i src/public/assets/css/connectfy/output.css -o src/public/assets/css/connectfy/tailwind.css --watch",
    "taiwind:build": "npx tailwindcss -i src/public/assets/css/connectfy/output.css -o src/public/assets/css/connectfy/tailwind.css",
    "connectfy:scss:watch": "sass --watch src/views/connectfy/style.scss src/views/connectfy/style.css",
    "connectfy:taiwind:watch": "npx tailwindcss -i src/views/connectfy/output.css -o src/views/connectfy/styles/tailwind.css --watch",
    "connectfy:taiwind:build": "npx tailwindcss -i src/views/connectfy/output.css -o src/views/connectfy/styles/tailwind.css",
    "connectdesk:scss:watch": "sass --watch src/views/connectdesk/style.scss src/views/connectdesk/style.css",
    "connectdesk:taiwind:watch": "npx tailwindcss -i src/views/connectdesk/output.css -o src/views/connectdesk/styles/tailwind.css --watch",
    "connectdesk:taiwind:build": "npx tailwindcss -i src/views/connectdesk/output.css -o src/views/connectdesk/styles/tailwind.css",
    "start:dev": "npm-run-all --parallel  scss:watch taiwind:watch prod:server",
    "start:macdev": "npm-run-all --parallel  scss:watch taiwind:watch prod:macserver",
    "connectfy:start:dev": "npm-run-all --parallel  connectfy:scss:watch connectfy:taiwind:watch prod:server",
    "connectfy:start:macdev": "npm-run-all --parallel  connectfy:scss:watch connectfy:taiwind:watch prod:macserver",
    "connectdesk:start:dev": "npm-run-all --parallel  connectdesk:scss:watch connectdesk:taiwind:watch prod:server",
    "connectdesk:start:macdev": "npm-run-all --parallel  connectdesk:scss:watch connectdesk:taiwind:watch prod:macserver",
    "build": "npx tsc",
    "copy:files": "copyfiles -u 2 src/views/**/* ../siteConnectDeploy/views",
    "copy:views": "copyfiles -u 2 src/views/**/**/* ../siteConnectDeploy/views",
    "copy:build": "npm run copy:files && npm run copy:views",
    "remove:views": "rm -rf ../siteConnectDeploy/views",
    "build:prod": "npm run build && npm run remove:views && npm run copy:build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@lottiefiles/dotlottie-web": "^0.43.0",
    "@whiskeysockets/baileys": "^6.7.9",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.34.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "gsap": "^3.13.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "qrcode-terminal": "^0.12.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.6",
    "@types/bcrypt": "^5.0.2",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.0",
    "@types/qrcode-terminal": "^0.12.2",
    "copyfiles": "^2.4.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.85.0",
    "tailwindcss": "^4.0.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}
